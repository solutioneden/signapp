<?xml version='1.0' encoding='utf-8'?>
<widget id="com.rnwholdings.*" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="Camera">
        <param name="browser-package" value="Camera" />
    </feature>
    <name>RNW SIGNAPP</name>
    <description>
        SignIn / SignOut Application for RNW
    </description>
    <author email="zhimeng.huang@rnwholdings.com" href="http://www.rnwholdings.com">
        RNW Holdings Developer
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
</widget>
